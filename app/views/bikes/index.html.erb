


<p id="notice"><%= notice %></p>

<h1>All Bikes for you</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Location</th>
      <th>Price/day/Euro</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @bikes.each do |bike| %>
      <tr>
        <td><%= bike.name %></td>
        <td><%= bike.description %></td>
        <td><%= bike.location %></td>
        <td><%= bike.price_per_day %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
   <td><%= link_to 'home', pages_home_path %></td>
   <td><%= link_to 'Creat a new bike', new_bike_path %></td>

